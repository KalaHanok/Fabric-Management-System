                                SELECT fabric_id, SUM(quantity*cost_price)/SUM(quantity) AS cost_price 
                                FROM purchases 
                                GROUP BY fabric_id
                                )
                                select fabrics.fabric_id, fabrics.fabric_name, fabrics.stock, COALESCE(latest_cost.cost_price*stock,0) as total_cost 
                                from fabrics left join latest_cost on fabrics.fabric_id = latest_cost.fabric_id;</sql><current_tab id="0"/></tab_sql></sqlb_project>
